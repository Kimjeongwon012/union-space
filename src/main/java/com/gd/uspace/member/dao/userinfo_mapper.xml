<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gd.uspace.member.dao.UserinfoDAO">
    <select id="getUserInfo" resultType="com.gd.uspace.member.dto.UserInfoDTO" parameterType="java.lang.String">
    SELECT user_id, user_pw, user_name, user_phone, user_gender, user_email
    FROM user
    WHERE user_id = #{param1}
</select>
</mapper>